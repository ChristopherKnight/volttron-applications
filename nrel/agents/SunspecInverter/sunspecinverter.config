{
	"agentid": "Sunspec",

    #Name for Inverter/DER client
    "Inverter Name": "Inverter1",

    # Required Device type
    #Possible values are TCP, RTU, MAPPED
    "device_type": "client.TCP",

    #Required for TCP device type: Device address
    "ip": "10.10.0.43",

    #Required for TCP device type: Port for communication
    #Defaults to 502
    "port": 502,

    #Required Modbus Slave Id
    "slave_id": 126,
    
    #Required for RTU, the name of the map file
    "name": "",

    #Required for RTU
    #Defaults to 9600
    "baudrate": 9600,

    #For RTU devices
    #Possible values PARITY_NONE, PARITY_EVEN.
    #Defaults to PARITY_NONE
    "parity": "PARITY_NONE",

    #Modbus request timeout(sec)
    "timeout": 6,
    
    #Optional Support files path
    "pathlist": ".",

    #PIN number for unique id 6-digit
    "pin": 654192,

    #Information about server IP 
    "server_IP": "190.62.1.3",

    #Server_port
    "server_port": 80,

    #DeviceCapability link, this link provided by the utility has links to function sets
    "device_capability_link": "/sep2/edev"
}

